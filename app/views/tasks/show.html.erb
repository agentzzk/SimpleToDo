<div class = "container">
<div class="task_nav">
  <%= link_to "Back", tasks_path, class: "btn btn-default" %>
</div>
<H2><%= @task.title%></H2>
<p>Last updated: <%= time_ago_in_words(@task.updated_at) %> ago</p>
<hr/>
<p><%= simple_format(@task.details, class: "lead text-left details container-fluid") %></p>
<p class="lead"><%= link_to "Edit", edit_task_path, class: "editLink" %></p> 
</div>